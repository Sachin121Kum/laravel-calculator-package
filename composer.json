{
    "name": "sachinkumar/starter-package",
    "description": "This is simple laravel package structure",
    "keywords": [
        "sachin121kum",
        "laravel-calculator-package"
    ],
    "homepage": "https://github.com/Sachin121Kum/laravel-calculator-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sachin Kumar",
            "email": "Sk31817@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Sachinkumar\\LaravelPackageStructure\\": "src/",
            "Sachinkumar\\LaravelPackageStructure\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SachinKumar\\LaravelPackageStructure\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit",
        "phpunit": "vendor/bin/phpunit",
        "test-coverage": "vendor\\bin\\phpunit --coverage-html test-reports/coverage",
        "format": "vendor\\bin\\php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SachinKumar\\LaravelPackageStructure\\LaravelPackageStructureServiceProvider"
            ],
            "aliases": {
                "LaravelPackageStructure": "SachinKUmar\\LaravelPackageStructure\\LaravelPackageStructureFacade"
            }
        }
    }
}
